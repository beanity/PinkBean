  "description": "A discord bot for MapleStory",
  "scripts": {
    "start": "node dist/index.js",
    "node": "node",
    "build": "tsc",
    "clean": "rm -rf dist",
    "formatAll": "prettier --write \"src/**/*.ts\"",
    "format": "prettier --write",
    "lint": "eslint . --ext .js,.ts",
    "watch-nodemon": "nodemon -L --watch dist --inspect=0.0.0.0:9229 --nolazy",
    "watch-tsc": "tsc && tsc -w",
    "test": "mocha --reporter dot --require ts-node/register test/**/*.ts"
  },
